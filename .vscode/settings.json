{
    "python.linting.enabled": false,
    "python.languageServer": "None",
    "files.associations": {
        "*.py": "python"
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true
    },
    "[python]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    }
}
